<div class="card" data-aos="fade-up">
  <div class="event_list">
    <div class="event_image col-lg-5">
        <a href="{{fields.view_node.content}}" title="{{fields.title.content}}">
			{{fields.field_image.content}}
	  	</a>  
    </div>
    <div class="card-body col-lg-7 my-auto">
      <div class="event_details">
      	<p class="date"><span><i class="fa fa-calendar" aria-hidden="true"></i></span>{{fields.field_date.content}}</p>
        <a href='{{fields.view_node.content}}'>
          <h3>{{fields.title.content}}</h3>
        </a>
        <p>{{fields.body.content}}</p>
       	<p class="campus"><span><i class="fa fa-map-marker" aria-hidden="true"></i></span>{{fields.field_campus_name.content}}</p>
      </div>
    </div>
  </div>
</div>
