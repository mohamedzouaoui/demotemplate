<div class="wrapper region region-content">
  {%- if title is not empty -%}
    <h2>{{ title }}</h2>
  {%- endif -%}
  {%- if items -%}
    <ol class="search-results node-results">
        {%- for item in items -%}
		        {{ item.value }}
		     {%- endfor -%}
		</ol>
{%- else -%}
	{{- empty -}}
{%- endif -%}

