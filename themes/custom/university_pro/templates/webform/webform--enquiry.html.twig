<form{{ attributes }}>
	<div class="row">
		<div class="col-lg-6">
			{{ element.elements.name }}
		</div>
		<div class="col-lg-6">
			{{ element.elements.email }}
		</div>
		<div class="col-lg-6">
			{{ element.elements.phone }}
		</div>
		<div class="col-lg-6">
			{{ element.elements.country }}
		</div>
		<div class="col-lg-12">
			{{ element.elements.message }}
		</div>
		<div class="col-lg-12">
			{{ element.elements.actions }}
		</div>     
	      {{ element.form_token }}
	      {{ element.form_build_id }}
	      {{ element.form_id }}
	      {{ message }}
	</div>
</form>
