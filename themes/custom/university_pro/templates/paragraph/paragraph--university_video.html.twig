<!-- University Video -->
<section id="university_video" class="tour_university_video" style="background-image: url({{ file_url(content.field_background_image['#items'].entity.uri.value)}})">
	<div class="overlay"></div>
	<div class="container">
		<div class="row">
			<div class="col-md-8 mx-auto" data-aos="fade-up">
				<div class="video_content galleryContainer">
					<h2>{{content.field_title.0}}</h2>
	                <p>{{content.field_description.0}}</p>
	                <a class="popup-youtube" href="{{content.field_link.0['#url']}}" title="{{content.field_link.0['#title']}}">
						<i class="fas fa-play"></i><span>{{content.field_link.0['#title']}}</span>
					</a>
				</div>
			</div>
		</div><!-- End of row -->
	</div><!-- End of container -->
</section>
<!-- End Of University Video -->