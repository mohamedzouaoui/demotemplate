<!-- University Life -->
<section id="university_life">
    <div class="container">
      <div class="row"> 
        <div class="col-12 col-sm-12 col-md-10 col-lg-6 section-image" data-aos="fade-up">
            <img src="{{ file_url(content.field_image['#items'].entity.uri.value)}}" alt="{{ content.field_image['#items'].alt }}" title="{{ content.field_image['#items'].title }}">
